{
  "name": "mapbox-map",
  "version": "1.0.0",
  "description": "Custom Mapbox map component for Retool",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf dist && rm -rf examples && npm install",
    "dev": "webpack-dev-server --mode=development",
    "dev-examples": "webpack-dev-server --mode=development --config webpack.examples.config.js",
    "prod": "webpack --mode=production",
    "prepare": "cd .. && husky install mapbox-map/.husky"
  },
  "keywords": [],
  "lint-staged": {
    "*.{ts,js,json,md,yaml}": "prettier --write"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.4.0",
    "file-loader": "^6.2.0",
    "husky": "^8.0.3",
    "prettier": "^3.1.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.59.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@mapbox/mapbox-gl-geocoder": "^5.0.1",
    "@mui/material": "^5.4.3",
    "lodash.isequal": "^4.5.0",
    "mapbox-gl": "^3.0.0",
    "mapboxgl-legend": "^1.14.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.3"
  },
  "overrides": {
    "@mapbox/mapbox-gl-geocoder": {
      "mapbox-gl": "$mapbox-gl"
    }
  }
}
